{
  "builds": [
    {
      "src": "phonebook_project/asgi.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": { "distDir": "staticfiles" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "phonebook_project/asgi.py"
    }
  ]
}
