{
  "version": 2,
  "builds": [
    {
      "src": "phonebook_project/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "phonebook_project/wsgi.py"
    }
  ]
    "env": {
    "PYTHONPATH": "./phonebook_project"
  }
}
