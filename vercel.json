{
    "builds": [{
        "src": "phonebook_project/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.13.1" }
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": { "distDir": "staticfiles" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "phonebook_project/wsgi.py"
        }
    ]
}
